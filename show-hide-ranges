Sub Worksheet_SelectionChange(ByVal Target As Excel.Range)
    If Range("$B$1") = "Show All" Then
    Call showall
    ElseIf Range("$B$1") = "Brand" Then
    Call showbrand
       ElseIf Range("$B$1") = "ItemName" Then
    ElseIf Range("$B$1") = "Item Name" Then
   Call showItemName
    
    
    ElseIf Range("$B$1") = "Selling Region" Then
   Call showSellingRegion
    
    
    ElseIf Range("$B$1") = "Channel" Then
   Call showChannel
    
    
    ElseIf Range("$B$1") = "Material UPC" Then
   Call showMaterialUPC
    
    
    ElseIf Range("$B$1") = "Engraving/Haz" Then
   Call showEngravingHaz
    
    
    ElseIf Range("$B$1") = "Hierarchy" Then
   Call showHierarchy
    
    
    ElseIf Range("$B$1") = "Material Dimensions" Then
   Call showMaterialDimensions
    
    
    ElseIf Range("$B$1") = "Material Weight" Then
   Call showMaterialWeight
    
    
    ElseIf Range("$B$1") = "Material Volume" Then
   Call showMaterialVolume
    
    
    ElseIf Range("$B$1") = "Material Label Claim" Then
   Call showMaterialLabelClaim
    
    
    ElseIf Range("$B$1") = "Inner Case Req" Then
   Call showInnerCaseReq
    
    
    ElseIf Range("$B$1") = "Units Per Inner" Then
   Call showUnitsPerInner
    
    
    ElseIf Range("$B$1") = "Inner Case GTIN" Then
   Call showInnerCaseGTIN
    
    
    ElseIf Range("$B$1") = "Inner Case Dims" Then
   Call showInnerCaseDims
    
    
    ElseIf Range("$B$1") = "Inner Case Weight" Then
   Call showInnerCaseWeight
    
    
    ElseIf Range("$B$1") = "Inner Case Vol" Then
   Call showInnerCaseVol
    
    
    ElseIf Range("$B$1") = "Outer Carton Req" Then
   Call showOuterCartonReq
    
    
    ElseIf Range("$B$1") = "Units Per Outer" Then
   Call showUnitsPerOuter
    
    
    ElseIf Range("$B$1") = "Outer Carton GTIN" Then
   Call showOuterCartonGTIN
    
    
    ElseIf Range("$B$1") = "Outer Carton Dims" Then
   Call showOuterCartonDims
    
    
    ElseIf Range("$B$1") = "Outer Carton Weight" Then
   Call showOuterCartonWeight
    
    
    ElseIf Range("$B$1") = "Outer Carton Vol" Then
   Call showOuterCartonVol
    
    
    ElseIf Range("$B$1") = "Pallet Req" Then
   Call showPalletReq
    
    
    ElseIf Range("$B$1") = "Pallet Arrangement" Then
   Call showPalletArrangement
    
    
    ElseIf Range("$B$1") = "Units Per Pallet" Then
   Call showUnitsPerPallet
    
    
    ElseIf Range("$B$1") = "Pallet Type" Then
   Call showPalletType
    
    
    ElseIf Range("$B$1") = "Pallet Dims" Then
   Call showPalletDims
    
    
    ElseIf Range("$B$1") = "Pallet Weight" Then
   Call showPalletWeight
    
    
    ElseIf Range("$B$1") = "Pallet Volume" Then
   Call showPalletVolume

 End If
End Sub

Sub showall()
Range("I:LR").EntireColumn.Hidden = False
End Sub

Sub showbrand()
Range("I:LR").EntireColumn.Hidden = True
Range("Brand").EntireColumn.Hidden = False
End Sub
Sub showItemName()
Range("I:LR").EntireColumn.Hidden = True
Range("ItemName").EntireColumn.Hidden = False
End Sub
Sub showSellingRegion()
Range("I:LR").EntireColumn.Hidden = True
Range("SellingRegion").EntireColumn.Hidden = False
End Sub
